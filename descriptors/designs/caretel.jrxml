	<queryString>
		<![CDATA[SELECT

max(case when meta_field_name_id=(SELECT  mfn.id FROM meta_field_name mfn,metafield_group_meta_field_map mgmfm,meta_field_group mfg,customer cu
   where  mfn.field_usage='ADDRESS1' and mfn.id = mgmfm.meta_field_value_id and
   mgmfm.metafield_group_id = mfg.id and mfg.account_type_id=cu.account_type_id and cu.user_id=cast($P{customerId} as INT)ORDER BY id ASC
LIMIT 1) then string_value end) as address1,

  max(case when meta_field_name_id=(SELECT mfn.id FROM meta_field_name mfn,metafield_group_meta_field_map mgmfm,meta_field_group mfg,customer cu
   where  mfn.field_usage='ORGANIZATION' and mfn.id = mgmfm.meta_field_value_id and
   mgmfm.metafield_group_id = mfg.id and mfg.account_type_id=cu.account_type_id and cu.user_id=cast($P{customerId} as INT)ORDER BY id ASC
LIMIT 1) then string_value end) as organisation,

 max(case when meta_field_name_id=(SELECT mfn.id FROM meta_field_name mfn,metafield_group_meta_field_map mgmfm,meta_field_group mfg,customer cu
   where  mfn.field_usage='STATE_PROVINCE' and mfn.id = mgmfm.meta_field_value_id and
   mgmfm.metafield_group_id = mfg.id and mfg.account_type_id=cu.account_type_id and cu.user_id=cast($P{customerId} as INT)ORDER BY id ASC
LIMIT 1) then string_value end)province,

	max(case when meta_field_name_id=(SELECT mfn.id FROM meta_field_name mfn,metafield_group_meta_field_map mgmfm,meta_field_group mfg,customer cu
   where  mfn.field_usage='ADDRESS2' and mfn.id = mgmfm.meta_field_value_id and
   mgmfm.metafield_group_id = mfg.id and mfg.account_type_id=cu.account_type_id and cu.user_id=cast($P{customerId} as INT)ORDER BY id ASC
LIMIT 1) then string_value end)address2,

   max(case when meta_field_name_id=(SELECT mfn.id FROM meta_field_name mfn,metafield_group_meta_field_map mgmfm,meta_field_group mfg,customer cu
   where  mfn.field_usage='CITY' and mfn.id = mgmfm.meta_field_value_id and
   mgmfm.metafield_group_id = mfg.id and mfg.account_type_id=cu.account_type_id and cu.user_id=cast($P{customerId} as INT)ORDER BY id ASC
LIMIT 1) then string_value end)as city,

   max(case when meta_field_name_id=(SELECT mfn.id FROM meta_field_name mfn,metafield_group_meta_field_map mgmfm,meta_field_group mfg,customer cu
   where  mfn.field_usage='POSTAL_CODE' and mfn.id = mgmfm.meta_field_value_id and
   mgmfm.metafield_group_id = mfg.id and mfg.account_type_id=cu.account_type_id and cu.user_id=cast($P{customerId} as INT)ORDER BY id ASC
LIMIT 1) then string_value end)as pincode

  FROM meta_field_value mfv,customer_account_info_type_timeline cuAcc ,customer cus,meta_field_group mfgg where mfv.id=cuAcc.meta_field_value_id
  and cuAcc.customer_id=cus.id and cus.user_id=cast($P{customerId} as INT) and cuAcc.account_info_type_id =mfgg.id and
  mfgg.account_type_id=cus.account_type_id ;]]>
